<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Empire</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
			
<!--VFEE WEAPONS-->			
			<li Class="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFEE_Gun_ChargeThumper"]</xpath>
					<value>
						<li Class="MuzzleFlash.MuzzleFlashProps">
							<def>MF_ChargedMuzzleFalsh</def>
							<drawSize>(1,1)</drawSize>
							<offsets>
								<li>(0.4,0)</li>
							</offsets>
						</li>
					</value>
				</li>
				
				
<!--VFEE TURRETS-->				
            <li Class="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFEE_Gun_StrikerTurret"]</xpath>
					<value>
						<li Class="MuzzleFlash.MuzzleFlashProps">
							<def>MF_StandardMuzzleFalsh</def>
							<drawSize>(3,3)</drawSize>
							<offsets>
								<li>(0.9,0)</li>
							</offsets>
						</li>
					</value>
				</li>
					
		    </operations>
		</match>
	</Operation>
</Patch>