<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
<!--VFEI PLASMA WEAPONS-->				
				<li Class="SafePatcher.PatchOperationSetModExtension">
					<xpath>Defs/ThingDef[defName="VFE_Gun_Plasmapiercer"]</xpath>
					<value>
						<li Class="MuzzleFlash.MuzzleFlashProps">
							<def>MF_StandardMuzzleFalshTwo</def>
							<drawSize>(2.5,1)</drawSize>
							<offsets>
								<li>(0.4,0.02)</li>
							</offsets>
						</li>
					</value>
				</li>
				
				<li Class="SafePatcher.PatchOperationSetModExtension">
					<xpath>Defs/ThingDef[defName="VFE_Gun_PlasmabeamRifle"]</xpath>
					<value>
						<li Class="MuzzleFlash.MuzzleFlashProps">
							<def>MF_StandardMuzzleFalsh</def>
							<drawSize>(2,1.3)</drawSize>
							<offsets>
								<li>(0.5,-0.01)</li>
							</offsets>
						</li>
					</value>
				</li>
				
				<li Class="SafePatcher.PatchOperationSetModExtension">
					<xpath>Defs/ThingDef[defName="VFE_Gun_PlasmaScattergun"]</xpath>
					<value>
						<li Class="MuzzleFlash.MuzzleFlashProps">
							<def>MF_StandardMuzzleFalsh</def>
							<drawSize>(1,2)</drawSize>
							<offsets>
								<li>(0.5,0)</li>
							</offsets>
						</li>
					</value>
				</li>
				
				
			
		    </operations>
		</match>
	</Operation>
</Patch>