<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AdaptiveStorageBase">
        <defName>MetalCrateC</defName>
        <label>Metal Crate C</label>
        <graphicData>
            <texPath>Metal_Crate/Metal_Crate_C</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(4,4)</drawSize>
            <drawRotated>false</drawRotated>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <description>A stack of metal crates that can store a range of items and is inflammable. Items stored inside will never deteriorate, burn, and don't affect the beauty of their surroundings.\n\nTotal Capacity: 64</description>
        <researchPrerequisites>
            <li>Machining</li>
        </researchPrerequisites>
        <fillPercent>0.2</fillPercent>
        <uiIconPath>Metal_Crate/Metal_Crate_C_UI</uiIconPath>
        <uiIconScale>1</uiIconScale>
        <size>(2,2)</size>
        <rotatable>false</rotatable>
        <statBases>
            <Beauty>0</Beauty>
            <MaxHitPoints>2000</MaxHitPoints>
            <WorkToBuild>6000</WorkToBuild>
            <Flammability>0</Flammability>
            <Mass>120</Mass>
        </statBases>
        <stuffCategories>
            <li>Metallic</li>
        </stuffCategories>
        <costStuffCount>200</costStuffCount>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
            <maxItemsInCell>16</maxItemsInCell>
            <fixedStorageSettings>
                <filter>
                    <categories>
                        <li>Root</li>
                    </categories>
                </filter>
            </fixedStorageSettings>
            <paintable>true</paintable>
        </building>
        <minifiedDef>MinifiedThing</minifiedDef>
        <thingCategories>
            <li>BuildingsFurniture</li>
        </thingCategories>
    </ThingDef>
</Defs>
