<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AdaptiveStorageBase">
        <defName>BarrelC</defName>
        <label>Barrel C</label>
        <graphicData>
            <texPath>Barrel/Barrel_C</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(3,3)</drawSize>
            <drawRotated>false</drawRotated>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <description>A pallet of wooden barrels and materials that can store a range of items. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nTotal Capacity: 32</description>
        <fillPercent>0.2</fillPercent>
        <researchPrerequisites>
            <li>ComplexFurniture</li>
        </researchPrerequisites>
        <uiIconPath>Barrel/Barrel_C_UI</uiIconPath>
        <uiIconScale>1</uiIconScale>
        <size>(2,2)</size>
        <rotatable>false</rotatable>
        <defaultPlacingRot>South</defaultPlacingRot>
        <statBases>
            <Beauty>0.7</Beauty>
            <MaxHitPoints>175</MaxHitPoints>
            <WorkToBuild>2250</WorkToBuild>
            <Flammability>1</Flammability>
            <Mass>54</Mass>
        </statBases>
        <stuffCategories>
            <li>Woody</li>
        </stuffCategories>
        <costStuffCount>135</costStuffCount>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
            <maxItemsInCell>8</maxItemsInCell>
            <fixedStorageSettings>
                <filter>
                    <categories>
                        <li>Root</li>
                    </categories>
                </filter>
            </fixedStorageSettings>
            <paintable>true</paintable>
        </building>
        <minifiedDef>MinifiedThing</minifiedDef>
        <thingCategories>
            <li>BuildingsFurniture</li>
        </thingCategories>
    </ThingDef>
</Defs>
