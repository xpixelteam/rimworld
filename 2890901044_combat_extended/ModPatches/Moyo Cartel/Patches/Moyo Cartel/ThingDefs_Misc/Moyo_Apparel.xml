	<!--Ballistic Suit-->

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Moyo_CBsuit"]/statBases</xpath>
		<value>
			<Bulk>25</Bulk>
			<WornBulk>5</WornBulk>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Moyo_CBsuit"]/statBases/Mass</xpath>
		<value>
			<Mass>12</Mass>
		</value>
