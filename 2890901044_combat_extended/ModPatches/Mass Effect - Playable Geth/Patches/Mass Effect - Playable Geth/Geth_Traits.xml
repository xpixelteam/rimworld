	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TraitDef[defName="GethSlowSpeed"]/degreeDatas/li/statOffsets</xpath>
		<value>
			<ArmorRating_Sharp>2</ArmorRating_Sharp>
