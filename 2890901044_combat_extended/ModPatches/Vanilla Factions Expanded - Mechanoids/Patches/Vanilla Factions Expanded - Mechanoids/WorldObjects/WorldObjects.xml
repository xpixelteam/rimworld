<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!--VFEM_Frigate-->
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/WorldObjectDef[defName="VFEM_Frigate"]/comps</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/WorldObjectDef[defName="VFEM_Frigate"]</xpath>
			<value>
				<comps/>
			</value>
		</nomatch>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/WorldObjectDef[defName="VFEM_Frigate"]/comps</xpath>
		<value>
			<li Class="CombatExtended.WorldObjects.WorldObjectCompProperties_Health">
				<healthModifier>2.6</healthModifier>
				<techLevelNoImpact>true</techLevelNoImpact>
				<chanceToNegateDamage>0.12</chanceToNegateDamage>
			</li>
			<li Class="CombatExtended.WorldObjects.WorldObjectCompProperties_Hostility"/>
		</value>
	</Operation>

