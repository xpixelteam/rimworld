<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- Sentinel -->
	<Operation Class="PatchOperationAdd">
		<xpath>
			Defs/BodyDef[defName="Sentinel"]//*[
			def="Sentinelbody"]
		</xpath>
		<value>
			<groups/>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>
			Defs/BodyDef[defName="Sentinel"]//*[
			def="Sentinelbody" or
			def="ForerunnerHead"]/groups
		</xpath>
		<value>
			<li>CoveredByNaturalArmor</li>
		</value>
	</Operation>

	<!-- Flood -->

	<Operation Class="PatchOperationAdd">
		<xpath>
			Defs/BodyDef[defName="Flood"]//*[
			def="FloodBody" or
			def="Nose"]
		</xpath>
		<value>
			<groups/>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>
			Defs/BodyDef[defName="Flood"]//*[
			def="FloodBody" or
			def="FloodHead" or
			def="Nose" or
			def="FloodMouth"]/groups
		</xpath>
		<value>
			<li>CoveredByNaturalArmor</li>
		</value>
	</Operation>

</Patch>
