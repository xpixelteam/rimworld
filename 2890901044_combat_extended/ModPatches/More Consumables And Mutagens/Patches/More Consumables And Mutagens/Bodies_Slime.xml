<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- === Bleed Rates === -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyPartDef[defName="SlimeBody"]</xpath>
		<value>
			<bleedRate>1.5</bleedRate>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/BodyPartDef[defName="SlimeHeart"]/bleedRate</xpath>
		<value>
			<bleedRate>18.75</bleedRate>
		</value>
	</Operation>

</Patch>