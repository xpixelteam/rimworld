<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- Antinium base trader -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TraderKindDef[defName="Ant_Base_Standard"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_Category">
				<categoryDef>Ammo</categoryDef>
				<thingDefCountRange>
					<min>0</min>
					<max>0</max>
				</thingDefCountRange>
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>WeaponsTurrets</categoryDef>
				<thingDefCountRange>
					<min>0</min>
					<max>0</max>
				</thingDefCountRange>
			</li>
			<li Class="StockGenerator_Animals">
				<tradeTagsSell>
					<li>CE_AnimalBoom</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>CE_AnimalBoom</li>
				</tradeTagsBuy>
				<kindCountRange>
					<min>1</min>
					<max>2</max>
				</kindCountRange>
				<countRange>
					<min>-2</min>
					<max>4</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>FSX</thingDef>
				<countRange>
					<min>10</min>
					<max>50</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Prometheum</thingDef>
				<countRange>
					<min>10</min>
					<max>50</max>
				</countRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>CE_PreIndustrialAmmo</tradeTag>
				<countRange>
					<min>60</min>
					<max>300</max>
				</countRange>
				<price>Cheap</price>
				<thingDefCountRange>
					<min>1</min>
					<max>4</max>
				</thingDefCountRange>
			</li>
		</value>
	</Operation>

