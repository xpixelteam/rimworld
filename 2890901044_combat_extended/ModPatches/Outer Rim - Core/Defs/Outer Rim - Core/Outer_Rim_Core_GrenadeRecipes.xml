<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- ========== New Recipes ========== -->

<RecipeDef ParentName="GrenadeRecipeBase">
	<defName>MakeORFragGrenade</defName>
	<label>make frag detonators x10</label>
	<description>Craft 10 frag detonators.</description>
	<jobString>Making frag detonators.</jobString>
	<researchPrerequisite>OuterRim_HypertechFabrication</researchPrerequisite>
	<workAmount>3600</workAmount>
	<ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>OuterRim_Durasteel</li>
				</thingDefs>
			</filter>
			<count>8</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>OuterRim_Tibanna</li>
				</thingDefs>
			</filter>
			<count>4</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>ComponentIndustrial</li>
				</thingDefs>
			</filter>
			<count>2</count>
		</li>
	</ingredients>
	<fixedIngredientFilter>
		<thingDefs>
			<li>OuterRim_Durasteel</li>
			<li>OuterRim_Tibanna</li>
			<li>ComponentIndustrial</li>
		</thingDefs>
	</fixedIngredientFilter>
	<products>
		<OuterRim_FragGrenade>10</OuterRim_FragGrenade>
	</products>
	<recipeUsers Inherit="False">
		<li>OuterRim_HypertechFabricator</li>
	</recipeUsers>
</RecipeDef>

