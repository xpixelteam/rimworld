<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- === Adds ammo to the scenario === -->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ScenarioDef[defName="VFED_NewSafehaven"]/scenario/parts</xpath>
		<value>
			<li Class="ScenPart_StartingThing_Defined">
				<def>ScatterThingsAnywhere</def>
				<thingDef>Ammo_10x18mmCharged</thingDef>
				<count>240</count>
			</li>
			<li Class="ScenPart_StartingThing_Defined">
				<def>ScatterThingsAnywhere</def>
				<thingDef>Ammo_40x68mmDemo_Thump</thingDef>
				<count>20</count>
			</li>
		</value>
	</Operation>

</Patch>