<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<!-- === Blood Wine === -->
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="DevDesigner_BloodWine"]/statBases</xpath>
					<value>
						<Bulk>1.5</Bulk>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>