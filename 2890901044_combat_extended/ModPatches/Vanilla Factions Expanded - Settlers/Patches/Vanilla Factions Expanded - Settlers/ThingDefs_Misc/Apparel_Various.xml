<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- === Vest === -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Apparel_Vest"]/statBases/StuffEffectMultiplierArmor</xpath>
		<value>
			<StuffEffectMultiplierArmor>2</StuffEffectMultiplierArmor>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Apparel_Vest"]/statBases</xpath>
		<value>
			<Bulk>3</Bulk>
			<WornBulk>1</WornBulk>
		</value>
	</Operation>

	<!-- === Great Dress === -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="Apparel_GreatDress"]/statBases/StuffEffectMultiplierArmor</xpath>
		<value>
			<StuffEffectMultiplierArmor>2</StuffEffectMultiplierArmor>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Apparel_GreatDress"]/statBases</xpath>
		<value>
			<Bulk>10</Bulk>
			<WornBulk>5</WornBulk>
		</value>
	</Operation>

</Patch>