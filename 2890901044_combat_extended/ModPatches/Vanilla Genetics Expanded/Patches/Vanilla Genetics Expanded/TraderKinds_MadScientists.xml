<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- Caravan Trader -->

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TraderKindDef[defName="GR_Outlander_CaravanTrader"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_SingleDef">
				<thingDef>FSX</thingDef>
				<countRange>
					<min>25</min>
					<max>100</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Prometheum</thingDef>
				<countRange>
					<min>15</min>
					<max>100</max>
				</countRange>
			</li>
		</value>
	</Operation>

	<!-- Orbital Trader -->

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TraderKindDef[defName="GR_OrbitalTrader"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_SingleDef">
				<thingDef>FSX</thingDef>
				<countRange>
					<min>50</min>
					<max>200</max>
				</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Prometheum</thingDef>
				<countRange>
					<min>50</min>
					<max>200</max>
				</countRange>
			</li>
		</value>
	</Operation>
</Patch>