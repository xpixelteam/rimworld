		<xpath>Defs/ThingDef[defName = "DarkDescent_Orb"]/statBases</xpath>
		<value>
			<Bulk>1.5</Bulk>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName = "DarkDescent_Vitae" or defName = "DarkDescent_Orgone"]/statBases</xpath>
		<value>
			<Bulk>0.5</Bulk>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[@Name = "DarkDescent_InactiveBase"]/statBases</xpath>
		<value>
			<Bulk>10</Bulk>
		</value>
	</Operation>
</Patch>