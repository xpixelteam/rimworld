<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AdaptiveStorageBase">
        <defName>CrateI</defName>
        <label>Crate I</label>
        <graphicData>
            <texPath>Crate/Crate_I</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(1,1)</drawSize>
            <drawRotated>false</drawRotated>
        </graphicData>
        <description>A crate that can store a range of items. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nTotal Capacity: 3\n\nRotate to change appearance.</description>
        <researchPrerequisites>
            <li>ComplexFurniture</li>
            <li>Machining</li>
        </researchPrerequisites>
        <fillPercent>0.2</fillPercent>
        <uiIconPath>Crate/Crate_I_UI</uiIconPath>
        <uiIconScale>1</uiIconScale>
        <size>(1,1)</size>
        <rotatable>true</rotatable>
        <defaultPlacingRot>South</defaultPlacingRot>
        <statBases>
            <Beauty>0</Beauty>
            <MaxHitPoints>50</MaxHitPoints>
            <WorkToBuild>450</WorkToBuild>
            <Flammability>0</Flammability>
            <Mass>10</Mass>
        </statBases>
        <costList>
            <Steel>15</Steel>
        </costList>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
            <maxItemsInCell>3</maxItemsInCell>
        </building>
        <minifiedDef>MinifiedThing</minifiedDef>
        <thingCategories>
            <li>BuildingsFurniture</li>
        </thingCategories>
    </ThingDef>
</Defs>
