<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AdaptiveStorageBase">
        <defName>CrateL</defName>
        <label>Crate L</label>
        <graphicData>
            <texPath>Crate/Crate_L</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(1,1)</drawSize>
            <drawRotated>false</drawRotated>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <description>A spacer crate that can store a range of items. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nTotal Capacity: 4\n\nRotate to change appearance.</description>
        <researchPrerequisites>
            <li>Machining</li>
        </researchPrerequisites>
        <fillPercent>0.2</fillPercent>
        <uiIconPath>Crate/Crate_L_UI</uiIconPath>
        <uiIconScale>1</uiIconScale>
        <size>(1,1)</size>
        <rotatable>true</rotatable>
        <statBases>
            <Beauty>1</Beauty>
            <MaxHitPoints>85</MaxHitPoints>
            <WorkToBuild>750</WorkToBuild>
            <Flammability>0</Flammability>
            <Mass>15</Mass>
        </statBases>
        <costList>
            <Steel>20</Steel>
        </costList>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
            <maxItemsInCell>4</maxItemsInCell>
        </building>
    </ThingDef>
</Defs>
