<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="AdaptiveStorageBase">
        <defName>ContainerC</defName>
        <label>Container C</label>
        <graphicData>
            <texPath>Container/Container_C</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(3,5)</drawSize>
            <drawRotated>false</drawRotated>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <description>A large shipping container that can store a range of items. Items stored inside will never deteriorate and don't affect the beauty of their surroundings.\n\nTotal Capacity: 180\n\nRotate to change appearance.</description>
        <researchPrerequisites>
            <li>Machining</li>
        </researchPrerequisites>
        <fillPercent>0.2</fillPercent>
        <uiIconPath>Container/Container_C_UI</uiIconPath>
        <uiIconScale>1</uiIconScale>
        <size>(3,5)</size>
        <rotatable>true</rotatable>
        <defaultPlacingRot>South</defaultPlacingRot>
        <statBases>
            <Beauty>0.7</Beauty>
            <MaxHitPoints>950</MaxHitPoints>
            <WorkToBuild>4500</WorkToBuild>
            <Flammability>0</Flammability>
            <Mass>500</Mass>
        </statBases>
		<costList>
			<Steel>450</Steel>
		</costList>
        <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
        <building>
            <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
            <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
            <maxItemsInCell>12</maxItemsInCell>
            <fixedStorageSettings>
                <filter>
                    <categories>
                        <li>Root</li>
                    </categories>
                </filter>
            </fixedStorageSettings>
            <paintable>true</paintable>
        </building>
        <minifiedDef>MinifiedThing</minifiedDef>
        <thingCategories>
            <li>BuildingsFurniture</li>
        </thingCategories>
    </ThingDef>
</Defs>
