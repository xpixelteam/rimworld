	<ThingDef ParentName="FurnitureBase">
		<defName>BreadMo_NutrientTap</defName>
		<label>nutrient solution tap</label>
		<description>A tap that dispenses nutrient solution into bottles.</description>
		<thingClass>Building_Storage</thingClass>
		<graphicData>
			<texPath>Things/Building/PipeSystem/NutrientSolutionOutlet</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<rotatable>false</rotatable>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.5</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<pathCost>50</pathCost>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Mass>8</Mass>
			<WorkToBuild>600</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>-2</Beauty>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Steel>60</Steel>
		</costList>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<fixedStorageSettings>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<thingDefs>
						<li>HydroponicNutrientSolution</li>
					</thingDefs>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Important</priority>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<thingDefs>
						<li>HydroponicNutrientSolution</li>
					</thingDefs>
				</filter>
			</defaultStorageSettings>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<designationCategory>NutrientSolutionPipeSystem</designationCategory>
		<staticSunShadowHeight>0.5</staticSunShadowHeight>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<comps>
			<li Class="PipeSystem.CompProperties_ConvertResourceToThing">
				<pipeNet>BreadMo_NutrientNetwork</pipeNet>
				<thing>HydroponicNutrientSolution</thing>
			</li>
		</comps>
		<researchPrerequisites>
			<li>AncientHydroponicTechniques</li>
		</researchPrerequisites>
	</ThingDef>

