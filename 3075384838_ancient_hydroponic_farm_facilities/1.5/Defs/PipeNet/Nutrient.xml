		</costList>
		<comps>
			<li Class="PipeSystem.CompProperties_ResourceStorage">
				<pipeNet>BreadMo_NutrientNetwork</pipeNet>
				<storageCapacity>100</storageCapacity>
				<barSize>(0.39, 0.09)</barSize>
				<margin>0.05</margin>
				<centerOffset>(-0.13, 0, 0.13)</centerOffset>
			</li>
		</comps>
		<building>
			<relatedBuildCommands>
				<li>BreadMo_NutrientPipe</li>
