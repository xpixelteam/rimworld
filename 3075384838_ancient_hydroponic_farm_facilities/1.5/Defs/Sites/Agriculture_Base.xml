<Defs>

 <!-- 隐藏的古老水培农场 -->
  <SitePartDef>
    <defName>Hidden_ancient_hydroponic_farm_Site</defName>
    <label>Hidden ancient hydroponic farm</label>
    <description>An ancient hydroponic farm that has been hidden for so long, we know very little about it.</description>
    <siteTexture>Icon/HiddenAncientHydroponicFarms</siteTexture>
    <expandingIconTexture>Icon/HiddenAncientHydroponicFarms</expandingIconTexture>
    <handlesWorldObjectTimeoutInspectString>true</handlesWorldObjectTimeoutInspectString>
    <tags>
      <li>Hidden_ancient_hydroponic_farm</li>
    </tags>
  </SitePartDef>
  <GenStepDef>
    <defName>Hidden_ancient_hydroponic_farm_GenStep</defName>
    <linkWithSite>Hidden_ancient_hydroponic_farm_Site</linkWithSite>
    <order>460</order>
    <genStep Class="KCSG.GenStep_CustomStructureGen">
      <structureLayoutDefs>
        <li>Hidden_ancient_hydroponic_farm</li>
      </structureLayoutDefs>
    </genStep>
  </GenStepDef>

