<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef  ParentName="AncientFacilities">
		<defName>SurvivalNutrientPackCrateA</defName>
		<label>SurvivalNutrientPack Crate</label>
		<description>The boxes of survival nutrient pastes pack stacked in the Crate，They are bound by tarpaulins and ropes, and perhaps breaking them can yield some resources</description>
		<altitudeLayer>Building</altitudeLayer>
		<rotatable>false</rotatable>
		<defaultPlacingRot>North</defaultPlacingRot>
		<graphicData>
			<texPath>Things/Building/Props/SurvivalNutrientPackCrateA</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,3)</drawSize>
			<drawOffset>(0,0,0)</drawOffset>
			<shadowData>
				<volume>(2, 0.8, 2)</volume>
			</shadowData>
		</graphicData>
		<size>(2,2)</size>
		<costList>
			<Steel>50</Steel>
			<MealNutrientPaste>30</MealNutrientPaste>
     	</costList>
		<killedLeavings>
			<ChunkSlagSteel>2</ChunkSlagSteel>
			<SurvivalNutrientPack>30</SurvivalNutrientPack>
		</killedLeavings>
	</ThingDef>

