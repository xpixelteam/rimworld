<?xml version="1.0" encoding="utf-8" ?>
<Defs>  

  <ThingDef ParentName="NonDeconstructibleAncientBuildingBase">
    <defName>VVE_ClassicCarWreck</defName>
    <label>classical car wreck</label>
    <description>An ancient, broken classic car. Everything that isn't rusted away was looted long ago. Given enough resources, it can be restored into a Springbok.</description>
    <rotatable>false</rotatable>
    <graphicData>
      <texPath>Things/Wrecks/Wreck_T3ClassicCar</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(3,5)</drawSize>
    </graphicData>
    <size>(2,4)</size>
    <fillPercent>0.5</fillPercent>
    <castEdgeShadows>false</castEdgeShadows>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <statBases>
      <MaxHitPoints>600</MaxHitPoints>
	  <Flammability>0</Flammability>
    </statBases>
    <killedLeavings>
      <ChunkSlagSteel>2</ChunkSlagSteel>
    </killedLeavings>
	<comps>
		<li Class="VanillaVehiclesExpanded.CompProperties_VehicleWreck">
			<vehicleDef>VVE_Springbok</vehicleDef>
			<researchDef>VVE_ClassicCars</researchDef>
			<spawnRotation>South</spawnRotation>
		</li>
	</comps>
  </ThingDef>
  
